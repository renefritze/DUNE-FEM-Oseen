# LOG_NONE = 1, LOG_ERR = 2, LOG_INFO = 4,LOG_DEBUG = 8,LOG_CONSOLE = 16,LOG_FILE = 32
# --> LOG_ERR | LOG_INFO | LOG_DEBUG | LOG_FILE = 46
loglevel: 22

#only prefix
logfile: dune_stokes
fem.io.datadir: data
fem.io.logdir: log
fem.io.datafileprefix: data_
fem.io.savestep: -1
fem.io.savecount: 1
fem.io.outputformat: 1
minref: 4
maxref: 0
minpow: -1
maxpow: 1
per-run-log-target: 0
per-run-output: 1

#
dgf_file_2d: grid_2d.dgf
dgf_file_3d: grid_3d.dgf

#
viscosity: 1
runtype: 0
relLimit: 1e-12
absLimit: 1e-10
inner_absLimit: 1e-10
use-cheat: 0
max_adaptions: 4
solverVerbosity: 0
h-factor: 1.0
eps: 1.0e-14
disableSolver: 0
diff-tolerance: 0.01
bfg-tau: 0.1
bfg-tau-start: 0
bfg-tau-stop: 0.5
bfg-tau-inc: 0.075
alternative-solve: 1
maxIter: 5000

