# IGNORE_HEADERS

LDADD = $(ALL_PKG_LDFLAGS) $(ALL_PKG_LIBS) $(LOCAL_LIBS)

noinst_PROGRAMS = laplace sinelaplace
laplace_SOURCES = laplace.cc
sinelaplace_SOURCES = sinelaplace.cc

GRIDTYPE=ALBERTAGRID
GRIDDIM=2
POLORDER=1
PROBLEM=SineProblem

AM_CPPFLAGS = $(ALL_PKG_CPPFLAGS) -D$(GRIDTYPE) -DGRIDDIM=$(GRIDDIM) -DPROBLEM=$(PROBLEM) -DPOLORDER=$(POLORDER)
AM_CXXFLAGS = -Wall -g
#AM_CXXFLAGS = -Wall -g -O3 -unroll -DNDEBUG
